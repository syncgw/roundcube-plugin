		"syncgw/mysql-bundle" : ">=9.19.80",
		"syncgw/roundcube-bundle" : ">=9.19.80",
		"syncgw/activesync-bundle" : ">=9.19.80"
	},
	"extra" : {
		"roundcube" : {
			"min-version" : "0.6.0",
			"max-version" : "10.21",
			"sql-dir" : "SQL"
